<!doctype html>
<html lang="pt">
    
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>
            ione Jardim Inteligente
        </title>
        <!-- Bootstrap -->
        <link href="css/bootstrap.css" rel="stylesheet">
        <link href="css/style.css" rel="stylesheet">
        <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.5.8/angular.min.js">
                
        </script>
        <script src="js/appController.js">
        </script>

    </head>
    
    <body ng-app="app" class="color_fundo">
        <div ng-controller="paginaPrincipalController">
            <nav class="navbar navbar-default bar_ajuste">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse"
                        data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                            <span class="sr-only">
                                Toggle navigation
                            </span>
                            <span class="icon-bar">
                            </span>
                            <span class="icon-bar">
                            </span>
                            <span class="icon-bar">
                            </span>
                        </button>
                        <a class="navbar-brand ajuste_icone"><img src="img/icone.png" alt="Dispute Bills">
        </a>
                    </div>
                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                        <ul class="nav navbar-nav navbar-right nav_dir">
                            <div class="navbar-form">
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="Email" ng-model="formData.emailLogin">
                                    <input type="password" class="form-control" placeholder="Senha" ng-model="formData.senhaLogin">
                                </div>
                                <button type="submit" class="btn btn-default" ng-click="login()">
                                    Login
                                </button>
                            </div>
                        </ul>
                    </div>
                </div>
            </nav>
            <div class="col-sm-12" id="inicio">
                <section id="principal">
                    <div class="col-sm-6">
                        <div class="papers text-center">
                            <img src="img/animacao.jpg" alt="">
                            <br/>
                            <a href="#"><b>Download my resume</b></a>
                            <h4 class="notopmarg nobotmarg">
                                John Vandeley
                            </h4>
                            <p>
                               
                            </p>
                        </div>
                    </div>


                    <div class="col-sm-6">
                        <!-- multistep form -->
                        <form id="msform" name="FormUsuarioJardim" ng-submit="gravaDadosUsuario()" data-toggle="validator" role="form">
                            <!-- progressbar -->
                            <ul id="progressbar">
                                <li class="active">
                                    Usuario
                                </li>
                                <li>
                                    Jardim
                                </li>
                                <li>
                                    Plantas
                                </li>
                                <li>
                                    Sensores
                                </li>
                                <li>
                                    Valvulas
                                </li>
                            </ul>
                            <!-- fieldsets -->


                            <fieldset>
                                <h2 class="fs-title">
                                    Crie sua Conta
                                </h2>
                                <br />

                                <span id="validacao"></span>

                                <div class="input-group">


                                   <input type="text" class="form-control" name="nome" id="nome" placeholder="Nome"
                                    ng-model="formData.nome" ng-disabled="campoNome"/>

                                    <input type="text" class="form-control" name="sobrenome" id="sobrenome" placeholder="Sobrenome" ng-model="formData.sobrenome" />

                                    <input type="email" class="form-control" name="email" id="email" placeholder="Email" ng-model="formData.email" />

                                    <input type="password" class="form-control" name="novaSenha" id="novaSenha" placeholder="Senha" ng-model="formData.senha" />

                                </div>

                                <input type="button" name="nextUsuario" id="nextUsuario" class=" action-button" value="Proximo"/>
                            </fieldset>



                            <fieldset>
                                <h2 class="fs-title">
                                    Crie seu Jardim
                                </h2>
                                <br />
                                <div class="col-sm-12">
                                    <div class="col-sm-3">
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="input-group">
                                            <input type="text" class="form-control" name="nomeJardim" id="nomeJardim" placeholder="Nome Jardim"
                                            ng-model="formData.nomeJardim" />
                                        </div>
                                    </div>
                                    <div class="col-sm-3">
                                    </div>
                                </div>
                                <input type="button" name="previous" class="previous action-button" value="Anterior"
                                />
                                <!--<input type="submit" name="submit" class="submit action-button" value="Criar"
                                ng-click="gravaDadosUsuario()"/>
                                -->
                                <input type="button" name="nextJardim" id="nextJardim" class=" action-button" value="Proximo"/>
                            </fieldset>
                            <fieldset>
                                <h2 class="fs-title">
                                    Escolha suas plantas
                                </h2>
                                <br />
                                <div class="input group">
                                    <div class="col-sm-12">
                                        <div class="col-sm-5">
                                            <div class="list-group">
                                                <a href="#" class="list-group-item active ajuste_select ajuste_color">
                                            Opções
                                          </a>
                                                <select multiple="multiple" id='listBoxPlanta' ng-model="formData.plantas">
                                                    <option ng-repeat="plantas in screenData.plantas" value="{{ plantas }}"
                                                    ng-selected="plantas==formData.plantas" class="list-group-item list-group-item-action">
                                                        {{plantas}}
                                                    </option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-sm-2 ajuste_botoes">
                                            <input type='button' class="btn btn-success" id='btnRightPlanta' value='  >  '
                                            />
                                            <br/>
                                            <input type='button' class="btn btn-success" id='btnLeftPlanta' value='  <  '
                                            />
                                        </div>
                                        <div class="col-sm-5">
                                            <div class="list-group">
                                                <a href="#" class="list-group-item active ajuste_select ajuste_color">
                                            Escolhido
                                          </a>
                                                <select multiple="multiple" id='listBoxPlantaResultado' name="listBoxPlantaResultado"
                                                ng-model="formData.plantas">
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <input type="button" name="previous" class="previous action-button" value="Anterior"
                                />
                                <input type="button" name="next" class="next action-button" value="Proximo"
                                />
                            </fieldset>
                            <fieldset>
                                <h2 class="fs-title">
                                    Escolha os Sensores
                                </h2>
                                <br />
                                <div class="input group">
                                    <div class="col-sm-12">
                                        <div class="col-sm-5">
                                            <div class="list-group">
                                                <a href="#" class="list-group-item active ajuste_select ajuste_color">
                                            Opções
                                          </a>
                                                <select multiple="multiple" id='listBoxSensores' ng-model="formData.sensores">
                                                    <option ng-repeat="sensores in screenData.sensores" value="{{ sensores }}"
                                                    ng-selected="sensores==formData.sensores" class="list-group-item list-group-item-action">
                                                        {{sensores}}
                                                    </option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-sm-2 ajuste_botoes">
                                            <input type='button' class="btn btn-success" id='btnRightSensores' value='  >  '
                                            />
                                            <br/>
                                            <input type='button' class="btn btn-success" id='btnLeftSensores' value='  <  '
                                            />
                                        </div>
                                        <div class="col-sm-5">
                                            <div class="list-group">
                                                <a href="#" class="list-group-item active ajuste_select ajuste_color">
                                            Escolhido
                                          </a>
                                                <select multiple="multiple" id='listBoxSensoresResultado' name="listBoxSensoresResultado"
                                                ng-model="formData.sensores">
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <input type="button" name="previous" class="previous action-button" value="Anterior"
                                />
                                <input type="button" name="next" class="next action-button" value="Proximo" />
                            </fieldset>
                            <fieldset>
                                <h2 class="fs-title">
                                    Escolha as Válvulas
                                </h2>
                                <br />
                                <div class="input group">
                                    <div class="col-sm-12">
                                        <div class="col-sm-5">
                                            <div class="list-group">
                                                <a href="#" class="list-group-item active ajuste_select ajuste_color">
                                            Opções
                                          </a>
                                                <select multiple="multiple" id='listBoxValvulas' ng-model="formData.valvulas">
                                                    <option ng-repeat="valvulas in screenData.valvulas" value="{{ valvulas }}"
                                                    ng-selected="valvulas==formData.valvulas" class="list-group-item list-group-item-action">
                                                        {{valvulas}}
                                                    </option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-sm-2 ajuste_botoes">
                                            <input type='button' class="btn btn-success" id='btnRightValvulas' value='  >  '
                                            />
                                            <br/>
                                            <input type='button' class="btn btn-success" id='btnLeftValvulas' value='  <  '
                                            />
                                        </div>
                                        <div class="col-sm-5">
                                            <div class="list-group">
                                                <a href="#" class="list-group-item active ajuste_select ajuste_color">
                                            Escolhido
                                          </a>
                                                <select multiple="multiple" id='listBoxValvulasResultado' name="listBoxValvulasResultado"
                                                ng-model="formData.valvulas">
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <input type="button" name="previous" class="previous action-button" value="Anterior"
                                />
                                <input type="submit" name="submit" class="submit action-button" value="Criar"/>

                            </fieldset>
                            <fieldset>
                                <h2 class="fs-title">
                                    Parabéns o seu Jardim foi criado com Sucesso
                                </h2>
                                <hr>
                                <br />
                                <div class="col-sm-12">
                                    <div class="col-sm-3">
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="img_padrao_card">
                                            <img src="trevo.png" class="img-responsive">
                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                        </form>
                    </div>
                </section>
            </div>
            <!--Name: <input type="text" ng-model="formData.nome" />
            <br />
            Name:
            <input type="text" ng-model="formData.sobre" />
            <br />
            <input type="button" class="btn btn-default" ng-click="gravaDadosSac()"
            value="Gravar" />
            <pre>
                {{formData | json}}
            </pre>
            -->
        </div>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js">
                    
        </script>
        <script src="js/bootstrap.js">
        </script>
        <script src="js/funcoes.js">
        </script>
        <script src="http://thecodeplayer.com/uploads/js/jquery.easing.min.js"
        type="text/javascript">
        </script>
    </body>

</html>